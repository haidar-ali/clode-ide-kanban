# Clode Studio Environment Configuration
# Copy this file to .env and update the values for your system

# ==================================================================
# OPTIONAL SETTINGS
# ==================================================================

# Application name displayed in the UI
APP_NAME=Clode Studio

# Set to development for debugging
NODE_ENV=development

# Port for Nuxt dev server (development only)
NUXT_PORT=3000

# Enable debug logging
DEBUG_LOGGING=false

# Enable Claude CLI debug mode
CLAUDE_DEBUG=false